package main 
  
import ( 
    "fmt"
    "math/rand"
) 
  

func intrandom(value_1, value_2 int) int { 
    return value_1 + value_2 + rand.Int() 
  
} 
   
func main() { 
  
   
    res1 := intrandom(2, 6) 
    res2 := intrandom(49, 50) 
    res3 := intrandom(120, 98) 
  
    
    fmt.Println("Result 1: ", res1) 
    fmt.Println("Result 2: ", res2) 
    fmt.Println("Result 3: ", res3) 
} 
